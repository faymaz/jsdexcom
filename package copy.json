{
  "name": "@faymaz/jsdexcom",
  "version": "1.0.0",
  "description": "Node.js library for accessing Dexcom Share API with international support",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node tests/test.js",
    "example": "node examples/basic-usage.js",
    "regions": "node examples/region-specific.js",
    "lint": "eslint src/**/*.js tests/**/*.js examples/**/*.js",
    "format": "prettier --write .",
    "docs": "jsdoc -c jsdoc.json",
    "prepare": "husky install",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "dexcom",
    "jsdexcom",
    "cgm",
    "diabetes",
    "glucose",
    "health",
    "medical",
    "share"
  ],
  "author": "faymaz <faymaz@faymaz.org> (https://github.com/faymaz)",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "clean-jsdoc-theme": "^4.2.17",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "jsdoc": "^4.0.2",
    "prettier": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faymaz/jsdexcom.git"
  },
  "bugs": {
    "url": "https://github.com/faymaz/jsdexcom/issues"
  },
  "homepage": "https://faymaz.github.io/jsdexcom",
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  }
}